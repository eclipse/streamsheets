  "scripts": {
    "start": "node scripts/start.js",
    "build": "cross-env REACT_APP_VERSION=$npm_package_version REACT_APP_BUILD_NUMBER=$TRAVIS_BUILD_NUMBER NODE_OPTIONS=--max_old_space_size=4096 node scripts/build.js",
    "build:langs": "babel scripts/translate.js | node",
    "test": "node scripts/test.js --env=jsdom",
    "local-build": "cross-env REACT_APP_VERSION=$npm_package_version REACT_APP_BUILD_NUMBER=$TRAVIS_BUILD_NUMBER yarn run build",
    "rpi-build": "cross-env REACT_APP_VERSION=$npm_package_version REACT_APP_BUILD_NUMBER=$TRAVIS_BUILD_NUMBER NODE_OPTIONS=--max_old_space_size=3044 node scripts/build.js",
    "docker-build": "yarn run build && docker build -t cedalo/webui:latest .",
    "lint": "./node_modules/.bin/eslint ."
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "jest-localstorage-mock"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      ".*([/\\\\]node_modules[/\\\\])*.*(jsg)|(parser)|(src).*(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules)[/\\\\]",
      "<rootDir>/scripts"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "bugs": {
    "url": "https://github.com/cedalo/streamsheets/issues"
  },
  "repository": {
    "url": "git+https://github.com/cedalo/streamsheets.git"
