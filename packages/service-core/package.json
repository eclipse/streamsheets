{
  "name": "@cedalo/service-core",
  "version": "2.5.0",
  "description": "A package providing basic service classes.",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "test": "./node_modules/.bin/jest --runInBand --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cedalo/streamsheets.git"
  },
  "author": "Cedalo AG",
  "bugs": {
    "url": "https://github.com/cedalo/streamsheets/issues"
  },
  "homepage": "https://github.com/cedalo/streamsheets/tree/master/packages/service-core",
  "dependencies": {
    "@cedalo/error-codes": "2.5.0",
    "@cedalo/id-generator": "2.5.0",
    "@cedalo/logger": "2.5.0",
    "@cedalo/messaging-client": "2.5.0",
    "@cedalo/protocols": "2.5.0",
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "cors": "2.8.4",
    "express": "4.17.1",
    "http-errors": "1.6.1",
    "mkdirp": "0.5.1",
    "morgan": "1.9.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "jest": "20.0.0",
    "jest-cli": "18.1.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "license": "EPL-2.0"
}
